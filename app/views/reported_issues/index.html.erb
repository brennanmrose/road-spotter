<h1>All Reported Issues</h1>
<% @reported_issues.map do |issue| %>
<ul>
	<li>
		<%= issue.chief_complaint %><br>
		<%= link_to 'Link to issue', category_reported_issue_path(issue.category, issue) %><br>
		Category: <%= link_to "#{issue.category_name}", category_path(issue.category_id) %>
	</li>
</ul>
<% end %>
<br>
<%= link_to 'Report a new issue', new_category_reported_issue_path %>